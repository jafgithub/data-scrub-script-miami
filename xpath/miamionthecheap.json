{
    "listpage": {
        "events_div": "//div[@class='entry-content mvt-content']",
        "events": "//h2[contains(text(), '{date_str}')]/following-sibling::div[contains(@class, 'row event')]",
        "event_url": ".//a[not(*)]/@href",
        "event_title": ".//a[not(*)]/text()"
    },
    "detailpage": {
        "event_image": "//div[@class='entry-content mvt-content']//img[1]/@data-src",
        "event_details" :  "//div[contains(@class, 'lotc-event-box')]",
        "start_datetime": "//div[contains(@class, 'lotc-event-box')][1]/div[@class='lotc-event-when']/div[@class='lotc-event-details']//text()",
        "ticket_price": "//div[contains(@class, 'lotc-event-box')][1]/div[@class='lotc-event-cost']/div[@class='lotc-event-details']//text()",
        "place_name": "//div[contains(@class, 'lotc-event-box')][1]/div[@class='lotc-event-where']/div[@class='lotc-event-details']//text()",
        "desc": "//div[@class='entry-content mvt-content']//img[1]//preceding::p[not(@class)]/text()",
        "location_info": "//div[@class='venue-info-box']",
        "address": "string((//div[@class='venue-info-box' and .//div[contains(@class, 'address-info')]])[1]//div[contains(@class, 'address-info')]/p[@itemprop='address'])",
        "address_url": "(//div[@class='venue-info-box' and .//div[contains(@class, 'address-info')]])//a[contains(text(), 'Get Directions')]/@href"
    }
}